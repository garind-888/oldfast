  author = {Terkelsen, Christian Juhl and S{\o}rensen, Jacob Thorsted and Maeng, Michael and Jensen, Lisette Okkels and Tilsted, Hans-Henrik and Trautner, Sven and Vach, Werner and Johnsen, S{\o}ren Paaske and Thuesen, Leif and Lassen, Jens Flensted},
